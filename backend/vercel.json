{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "env": {
    "TMDB_API_KEY": "@tmdb_api_key",
    "NODE_ENV": "production",
    "FRONTEND_URL": "@frontend_url"
  }
}
